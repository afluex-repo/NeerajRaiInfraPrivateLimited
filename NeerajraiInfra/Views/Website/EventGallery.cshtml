@model NeerajraiInfra.Models.Website

@{
    ViewBag.Title = "Events";
    Layout = "~/Views/Shared/WebsiteLayout.cshtml";
}

  <!-- Start Page Banner Area -->
<div class="page-banner-area">
    <div class="container">
        <div class="page-banner-content wow animate__animated animate__fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">
            <h2>Gallery</h2>
            <ul>
                <li>
                    <a href="~/Website/Index">Home</a>
                </li>
                <li>Gallery</li>
            </ul>
        </div>
    </div>

    <div class="page-banner-image wow animate__animated animate__fadeInRightight" data-wow-delay="300ms" data-wow-duration="2000ms">
        <img src="/WebsiteCss/assets/images/page-banner.png" alt="image">
    </div>
</div>
<!-- End Page Banner Area -->
<!-- Start Gallery Area -->
<section class="gallery-area pt-100 pb-70">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_1.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_1.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_2.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_2.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_3.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_3.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_4.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_4.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_5.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_5.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6">
                <div class="single-gallery-item">
                    <a data-fancybox="gallery" href="/WebsiteCss/assets/images/main-slides/banner_6.jpg">
                        <img src="/WebsiteCss/assets/images/main-slides/banner_6.jpg" class="project_img" alt="image">
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Gallery Area -->




