@model NeerajraiInfra.Models.DashBoard

@{
    ViewBag.Title = "EmployeeDashboard";
    Layout = "~/Views/Shared/_EmpLayout.cshtml";
}
<html lang="en">
<body class="nav-md">
    @using (Html.BeginForm("EmployeeDashboard", "Admin", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
    {
        <section class="section">
            <div class="section-body">
                <div class="row mt-sm-4">
                    <div class="col-12 col-md-12 col-lg-12">
                        <div class="card author-box">
                            <div class="card-body d-lg-flex align-content-center" >
                                <div class="author-box-center">
                                    <img alt="image" src="/WebsiteCss/assets-old/images/Neerajrai_logo.jpg" class="rounded-circle author-box-picture">
                                    <div class="clearfix"></div>
                                    <div class="author-box-name">
                                        <a href="#">NeerajRaiInfra</a>
                                    </div>
                                    
                                </div>
                                <div class="text-center pl-lg-5">
                                    <div class="author-box-description">
                                        <p style="line-height:3; font-size:30px; font-weight:bold; color:#1c1e46;" >
                                            @Session["Name"] Now you Are NeerajRaiInfra Employee.
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>

                </div>
            </div>
        </section>
    }
</body>
</html>
